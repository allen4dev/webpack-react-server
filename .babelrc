{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 versions",
          "node": "current"
        },
        "modules": false
      }
    ],
    "flow",
    "react"
  ],
  "plugins": ["react-hot-loader/babel", "transform-class-properties"],
  "env": {
    "test": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  }
}
